{
    "name": "dmykos/ip-store-bundle",
    "description": "Ip Store Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Kosto",
            "email": "dmykos@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "symfony/framework-bundle": "^3.4 || ^4.4",
        "symfony/validator": "^3.4 || ^4.4",
        "symfony/orm-pack": "*"
    },
    "autoload": {
        "psr-4": {
